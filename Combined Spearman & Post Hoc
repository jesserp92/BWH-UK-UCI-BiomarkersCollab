  dplyr::mutate(residuals = resid(bamy40.lm ),
                standardized.residuals = rstandard(bamy40.lm ),
                studentized.residuals = rstudent(bamy40.lm ),
                cooks.distance = cooks.distance(bamy40.lm ),
                dffit = dffits(bamy40.lm ),
