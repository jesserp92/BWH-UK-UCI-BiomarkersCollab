source("https://slcladal.github.io/rscripts/SampleSizeMLR.r")
source("https://slcladal.github.io/rscripts/ExpR.r")
# check if sample size is sufficient
smplesz(nt1taurpt.lm)

# check beta-error likelihood
expR(nt1taurpt.lm)

